<body id="myPage" ng-app="myApp" ng-controller="homeCtrl" data-spy="scroll" data-target=".navbar" data-offset="60">
<div class="preloader">
  <ul class="circle">
  <li class="point"></li>
  <li class="point"></li>
  <li class="point"></li>
  <li class="point"></li>
  <li class="point"></li>
